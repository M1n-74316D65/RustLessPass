{
  "productName": "RustedLessPass",
  "version": "1.1.0",
  "identifier": "org.rustedlesspass.dev",
  "build": {
    "beforeDevCommand": "trunk serve",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "trunk build --release",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "RustedLessPass",
        "width": 501.302,
        "height": 638.628,
        "minWidth": 501.302,
        "minHeight": 638.628,
        "resizable": true
      }
    ],
    "security": {
      "csp": "default-src 'self'; img-src 'self' data:; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; font-src 'self' data:"
    }
  },
  "bundle": {
    "active": true,
    "category": "Utility",
    "targets": "all",
    "icon": [
      "../assets/favicon.png",
      "../assets/favicon.icns",
      "../assets/favicon.ico"
    ]
  }
}
